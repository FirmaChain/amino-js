        "eslint":                           "5.16.0",
        "eslint-loader":                    "2.1.2",
        "eslint-plugin-node":               "9.0.1",
        "jest":                             "24.8.0",
        "ts-jest":                          "24.0.2",
        "ts-loader":                        "6.0.0",
        "ts-node":                          "8.1.0",
